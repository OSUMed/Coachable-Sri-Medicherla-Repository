class Solution:
    def isCompleteTree(self, root: Optional[TreeNode]) -> bool:
        queue = collections.deque([root])
        nullFlag = False
        while queue:
            node = queue.popleft()
            if node:
                if nullFlag: 
                    return False
                queue.append(node.left)
                queue.append(node.right)

            else:
                nullFlag = True
        return True
        
